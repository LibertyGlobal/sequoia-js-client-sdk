{
  "name": "@pikselpalette/sequoia-js-client-sdk",
  "version": "0.1.0",
  "description": "Sequoia client SDK for Javascript",
  "main": "dist/sequoia-client.js",
  "scripts": {
    "prepublishOnly": "NODE_ENV=production webpack --bail --progress --profile --optimize-minimize",
    "build": "webpack --bail --progress --profile",
    "doc": "jsdoc -c jsdoc.conf.json -r lib -p -R README.md -P package.json --verbose --pedantic",
    "lint": "eslint lib && eslint test/spec",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:mutate": "NODE_ENV=test stryker run"
  },
  "jest": {
    "testMatch": [
      "**/test/spec/**/*.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "coverageDirectory": "test/results",
    "setupFiles": [
      "./test/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.piksel.com:experiencements/sequoia-client-sdk.git"
  },
  "keywords": [
    "sequoia",
    "es6",
    "javascript",
    "sdk"
  ],
  "author": "Piksel Product",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jest": "^21.15.0",
    "exports-loader": "^0.7.0",
    "fetch-mock": "^6.3.0",
    "imports-loader": "^0.8.0",
    "ink-docstrap": "^1.3.2",
    "jest": "^22.4.3",
    "jest-fetch-mock": "^1.5.0",
    "jsdoc": "^3.5.5",
    "json-loader": "^0.5.7",
    "request": "^2.85.0",
    "stryker": "^0.21.1",
    "stryker-api": "^0.15.0",
    "stryker-babel-transpiler": "^0.4.0",
    "stryker-html-reporter": "^0.13.0",
    "stryker-javascript-mutator": "^0.5.0",
    "stryker-jest-runner": "^0.6.0",
    "webpack": "2.2.0"
  },
  "dependencies": {
    "query-string": "^6.0.0"
  },
  "publishConfig": {
    "registry": "https://npmjs.org"
  }
}
